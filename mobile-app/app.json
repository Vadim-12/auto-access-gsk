{
	"expo": {
		"name": "mobile-app-2",
		"slug": "mobile-app-2",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/images/icon.png",
		"scheme": "mobileapp2",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/images/favicon.png"
		},
		"plugins": ["expo-router", ["expo-splash-screen", { "root": "./src/app" }]],
		"experiments": {
			"typedRoutes": true
		}
	}
}
